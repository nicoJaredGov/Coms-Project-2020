<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project2020">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.newproject2020.TestActivity" />
        <activity android:name="com.example.newproject2020.orders.AddOrderEmployeeActivity" />
        <activity android:name="com.example.newproject2020.orders.AddOrderCustomerActivity" />
        <activity android:name="com.example.newproject2020.customer.CustomerRegistrationActivity" />
        <activity android:name="com.example.newproject2020.employee.EmployeeRegistrationActivity" />
        <activity android:name="com.example.newproject2020.customer.CustomerActivity" />
        <activity android:name="com.example.newproject2020.employee.EmployeeActivity" />
        <activity android:name="com.example.newproject2020.CustomerLoginActivity" />
        <activity android:name="com.example.newproject2020.EmployeeLoginActivity" />
        <activity android:name="com.example.newproject2020.UserActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>